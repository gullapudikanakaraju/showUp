<!DOCTYPE html>
<html>
<head>
    <title>Localities of Different Properties posted by Dealer</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
    <script src="https://cdn.socket.io/socket.io-1.4.5.js"></script>   
</head>
<body>
    <h1 style="text-align: center">Your are a part of the following rooms :</h1>
    <h3 style="text-align: center" id="status">offline</h3>
    <div id="room_links"></div>
</body>

<script>
    console.log('hello');
    var socket = io.connect('http://localhost:5555');
    temp_socket = socket;
    socket.on('connect', function(){
        console.log('connection established');
        document.getElementById('status').textContent = 'online';

        socket.emit('getMyRooms');

        socket.on('getMyRooms_reply', function(data){
            console.log('my rooms are ', data);
        });

        socket.on('newGroupMessage', function(data){
            console.log('new group message is ', data);
        });

        socket.on('disconnect', function(){
          console.log('connection lost');
          document.getElementById('status').textContent = 'offline';
        });
    });

</script>
</html>